{
  "name": "mcp-webdriverio",
  "version": "1.0.1",
  "description": "WebdriverIO MCP Server",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/lib/server.js",
  "bin": {
    "mcp-webdriverio": "dist/bin/mcp-webdriverio.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/bin/mcp-webdriverio.js",
    "dev": "tsc -w",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "cd tests && node --experimental-vm-modules ../node_modules/jest/bin/jest.js",
    "test:watch": "cd tests && node --experimental-vm-modules ../node_modules/jest/bin/jest.js --watch",
    "test:coverage": "cd tests && node --experimental-vm-modules ../node_modules/jest/bin/jest.js --coverage"
  },
  "keywords": [
    "mcp",
    "webdriverio",
    "automation",
    "testing",
    "browser",
    "selenium"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "webdriverio": "^8.32.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@wdio/types": "^8.32.5",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "@jest/globals": "^29.7.0"
  },
  "engines": {
    "node": ">=18"
  }
}
